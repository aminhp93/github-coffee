{
  "name": "github-coffee",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "build-storybook": "rimraf public/storybook-static && storybook build --output-dir public/storybook-static",
    "check-eslint-disable": "./scripts/check-eslint-disable.sh",
    "dev": "next dev --turbopack",
    "lint": "next lint && pnpm check-eslint-disable",
    "prettier-fix": "prettier --write .",
    "prettier-check": "prettier --check .",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --run",
    "test:related": "vitest ./tests/unit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.5",
    "axios": "^1.7.9",
    "gray-matter": "^4.0.3",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@next/bundle-analyzer": "^15.2.4",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/nextjs": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@tailwindcss/postcss": "^4.0.7",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.20.1",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-storybook": "^0.11.4",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "storybook": "^8.6.4",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
